{
  "name": "netease-meeting",
  "version": "3.14.1",
  "private": true,
  "description": "Secure, simple and open video conferencing solution that enhances office collaboration efficiency and caters to meetings of all sizes. Features include remote audio/video conferences and online collaboration tools.",
  "author": "netease",
  "main": "./app/main.js",
  "scripts": {
    "build": "cross-env PLATFORM=electron RUN_ENV=development umi build",
    "build:h5": "cross-env PLATFORM=h5 RUN_ENV=development umi build",
    "build:web": "cross-env RUN_ENV=development umi build",
    "build:prod": "cross-env PLATFORM=electron RUN_ENV=production umi build",
    "build:h5-prod": "cross-env PLATFORM=h5 RUN_ENV=production umi build",
    "build:web-prod": "cross-env RUN_ENV=production umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "start": "umi dev",
    "start:h5": "cross-env PLATFORM=h5 umi dev",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "ansi-regex": "^6.0.1",
    "antd": "^5.4.7",
    "antd-mobile": "^5.18.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.1",
    "hawk-web": "^1.3.14",
    "react": "17.x",
    "react-dom": "17.x",
    "react-qr-code": "^2.0.12",
    "swiper": "8",
    "umi": "^3.5.40",
    "wait-on": "^7.0.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.40",
    "lint-staged": "^10.0.7",
    "node-loader": "^2.0.0",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
