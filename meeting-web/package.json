    "ts-md5": "^1.2.7",
    "v-tooltip": "2.0.2",
    "vue": "2.6.11",
    "vue-i18n": "8.22.4",
    "vue-router": "3.4.6",
    "vue2-animate": "2.1.4",
    "vuex": "3.4.0",
    "yunxin-log-debug": "^1.1.4"
  },
