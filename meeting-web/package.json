  "license": "MIT",
  "main": "publish/neWebMeeting.umd.min.js",
  "module": "publish/neWebMeeting.umd.min.js",
  "private": true,
  "workspaces": {
    "nohoist": [
      "@vue/**",
      "typescript",
      "kit-chatroom-web",
      "vue",
      "vue/**",
      "vue-i18n",
      "@typescript-eslint",
      "vue-router",
      "vuex",
      "vue-router"
    ]
  },
  "files": [
    "publish"
  ],
  "keywords": [
    "meeting",
    "netease",
    "rtc"
  ],
  "scripts": {
    "serve": "vue-cli-service serve --mode local",
    "serve-website-dev": "vue-cli-service serve  --mode website-local-dev src/website.ts",
    "serve-website": "vue-cli-service serve  --mode website-local-online src/website.ts",
    "build-website-dev": "vue-cli-service build  --mode website-build-dev src/website.ts",
    "build-website": "vue-cli-service build  --mode website-build-online src/website.ts",
    "build-lib": "vue-cli-service build --target lib --name neWebMeeting",
    "build-lib-inline": "vue-cli-service build --target lib --inline-vue --no-clean --mode production --name NEMeetingKit src/umd.ts",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@netease-yunxin/log-storage": "^1.0.5",
    "antd": "^4.16.13",
